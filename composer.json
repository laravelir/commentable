{
    "name": "laravelir/commentable",
    "description": "a commenting package for laravel",
    "homepage": "https://github.com/laravelir/commentable",
    "type": "library",
    "version": "0.1.6",
    "keywords": [
        "laravel",
        "laravel-package",
        "laravel support",
        "lumen packages",
        "laravel packages",
        "laravel comment",
        "comments"
    ],
    "authors": [
        {
            "name": "miladimos",
            "email": "miladimos@outlook.com",
            "role": "maintainer",
            "homepage": "https://github.com/miladimos"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravelir\\Commentable\\": "src/"
        },
        "files": [
            "./src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Laravelir\\Commentable\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravelir\\Commentable\\Providers\\CommentableServiceProvider"
            ],
            "aliases": {
                "Commentable": "Laravelir\\Commentable\\Facades\\Commentable"
            }
        }
    },
    "require": {
        "php": ">=7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "minimum-stability": "stable",
    "license": "MIT"
}
